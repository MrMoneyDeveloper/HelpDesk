@model C8.eServices.Mvc.Models.Client

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
<!doctype html>
<html lang="en">
<head>
    <link href="~/assets/libs/jsvectormap/css/jsvectormap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/css/bootstrap.min.css" id="bootstrap-style" rel="stylesheet" type="text/css" />
    <link href="~/assets/css/icons.min.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/css/app.min.css" id="app-style" rel="stylesheet" type="text/css" />



    <link href="~/assets/libs/choices.js/public/assets/styles/choices.min.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/libs/flatpickr/flatpickr.min.css" rel="stylesheet">

    <!-- One of the following themes -->
    <link rel="stylesheet" href="~/assets/libs/simonwep/pickr/themes/classic.min.css" /> <!-- 'classic' theme -->
    <link rel="stylesheet" href="~/assets/libs/simonwep/pickr/themes/monolith.min.css" /> <!-- 'monolith' theme -->
    <link rel="stylesheet" href="~/assets/libs/simonwep/pickr/themes/nano.min.css" /> <!-- 'nano' theme -->
    <!-- Include the flatpickr styles from CDN -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/flatpickr/dist/flatpickr.min.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@24,100,0,-25" />


    <style>
        .material-icons {
            font-family: 'Material Icons';
            font-weight: normal;
            font-style: normal;
            font-size: 24px; /* Preferred icon size */
            display: inline-block;
            line-height: 1;
            text-transform: none;
            letter-spacing: normal;
            word-wrap: normal;
            white-space: nowrap;
            direction: ltr;
            /* Support for all WebKit browsers. */
            -webkit-font-smoothing: antialiased;
            /* Support for Safari and Chrome. */
            text-rendering: optimizeLegibility;
            /* Support for Firefox. */
            -moz-osx-font-smoothing: grayscale;
            /* Support for IE. */
            font-feature-settings: 'liga';
        }
    </style>

</head>
<body>
    <div class="main-content">

        <div class="page-content">
            <div class="container-fluid">

                <!-- start page title -->
                <div class="row">
                    <div class="col-12">
                        <div class="page-title-box d-flex align-items-center justify-content-between">
                            @*<h4 class="mb-0">Client Profile</h4>*@

                            <div class="page-title-right">
                                @*<ol class="breadcrumb m-0">
                                    <li class="breadcrumb-item"><a href="javascript: void(0);">Signup</a></li>
                                    <li class="breadcrumb-item active">Onboarding Request</li>
                                </ol>*@
                            </div>

                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xxl-12 col-xl-6" style="width:100%; height: 100%;">
                        <div class="card">
                            <div class="card-header justify-content-between d-flex align-items-center">
                                <div class="text-center">
                                    <h4 class="card-title mb-0">Client Onboarding Information</h4>
                                </div>

                            </div><!-- end card header -->
                            <div class="card-body">
                                <!-- Nav tabs -->
                                <ul class="nav nav-tabs" role="tablist">
                                    <li class="nav-item">
                                        <a class="nav-link active" data-bs-toggle="tab" href="#navtabs-home" role="tab">
                                            <span class="d-block d-sm-none"><i class="fas fa-home"></i></span>
                                            <span class="d-none d-sm-block" style="color: black;">Client Information</span>

                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" data-bs-toggle="tab" href="#navtabs-profile" role="tab">
                                            <span class="d-block d-sm-none"><i class="far fa-user"></i></span>
                                            <span class="d-none d-sm-block" style="color:black;">Onboard Entity User</span>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" data-bs-toggle="tab" href="#navtabs-messages" role="tab">
                                            <span class="d-block d-sm-none"><i class="far fa-envelope"></i></span>
                                            <span class="d-none d-sm-block" style="color:black;">@ViewBag.ClientInfo.Company Users</span>
                                        </a>
                                    </li>
                                </ul>

                                <!-- Tab panes -->
                                <div class="tab-content p-3 text-muted">
                                    <div class="tab-pane active" id="navtabs-home" role="tabpanel">
                                        <div class="card-body">
                                            <div class="row">
                                                <div class="col-xl-6">

                                                    <div class="mb-3 row">
                                                        <label for="" class="col-md-2 col-form-label" style="width:fit-content">Company Name:</label>
                                                        <div class="col-md-9 input-group">
                                                            <span class="input-group-text material-symbols-outlined"> corporate_fare</span>
                                                            <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default" value="@ViewBag.ClientInfo.Company">
                                                        </div>
                                                    </div><!-- end row -->







                                                    <div class="mb-3 row">
                                                        <label for="" class="col-md-2 col-form-label" style="width:fit-content">Industry:</label>
                                                        <div class="col-md-9 input-group">
                                                            <span class="input-group-text material-symbols-outlined"> warehouse</span>
                                                            <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default" value="@ViewBag.ClientInfo.Industr">
                                                        </div>
                                                    </div><!-- end row -->

                                                    <div class="mb-3 row">
                                                        <label for="" class="col-md-2 col-form-label" style="width:fit-content">Domain:</label>
                                                        <div class="col-md-9 input-group">
                                                            <span class="input-group-text material-symbols-outlined"> domain</span>
                                                            <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default" value="@ViewBag.ClientInfo.Domain">
                                                        </div>
                                                    </div><!-- end row -->


                                                    <div class="mb-3 row">
                                                        <label for="" class="col-md-2 col-form-label" style="width:fit-content">Access Period:</label>
                                                        <div class="col-md-9 input-group">
                                                            <span class="input-group-text material-symbols-outlined"> calendar_clock</span>
                                                            <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default" value="@ViewBag.ClientInfo.PeriodTo">
                                                        </div>
                                                    </div><!-- end row -->


                                                    <div class="mb-3 row">
                                                        <label for="" class="col-md-2 col-form-label" style="width:fit-content">Client Operational Hour:</label>
                                                        <div class="col-md-9 input-group">
                                                            <span class="input-group-text material-symbols-outlined"> schedule</span>
                                                            <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default" value="@ViewBag.ClientInfo.OperationalHours">
                                                        </div>
                                                    </div><!-- end row -->





                                                    <div class="mb-3 row">
                                                        @*<label for="" class="col-md-2 col-form-label" style="width:fit-content">Client Person of Contact</label>*@
                                                        <h4 class="mb-0" style="width:fit-content">@ViewBag.ClientInfo.Company Allocated Person of Contact</h4>
                                                    </div><!-- end row -->



                                                    <div class="mb-3 row">
                                                        <label for="" class="col-md-2 col-form-label" style="width:fit-content">Title Of Person:</label>
                                                        <div class="col-md-9 input-group">
                                                            <span class="input-group-text material-symbols-outlined"> settings_accessibility</span>
                                                            <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default" value="@ViewBag.ClientRep.Title">
                                                        </div>
                                                    </div><!-- end row -->


                                                    <div class="mb-3 row">
                                                        <label for="" class="col-md-2 col-form-label" style="width:fit-content">Phone Number:</label>
                                                        <div class="col-md-9 input-group">
                                                            <span class="input-group-text material-symbols-outlined"> call</span>
                                                            <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default" value="@ViewBag.ClientRep.MobileNumber">
                                                        </div>
                                                    </div><!-- end row -->




                                                    <div class="mb-3 row">
                                                        <label for="" class="col-md-2 col-form-label" style="width:fit-content">Designation:</label>
                                                        <div class="col-md-9 input-group">
                                                            <span class="input-group-text material-symbols-outlined"> badge</span>
                                                            <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default" value="@ViewBag.ClientRep.Designation">
                                                        </div>
                                                    </div><!-- end row -->







                                                </div><!-- end col -->

                                                <div class="col-xl-6">
                                                    <div class="mb-3 row">
                                                        <label for="" class="col-md-2 col-form-label" style="width:fit-content">Country:</label>
                                                        <div class="col-md-9 input-group">
                                                            <span class="input-group-text material-symbols-outlined"> flag</span>
                                                            <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default" value="@ViewBag.ClientInfo.Countr">
                                                        </div>
                                                    </div><!-- end row -->



                                                    <div class="row mb-3">
                                                        <label for="" class="col-md-2 col-form-label" style="width:fit-content">VAT No.:</label>
                                                        <div class="col-md-9 input-group">
                                                            <span class="input-group-text material-symbols-outlined"> percent</span>
                                                            <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default" value="@ViewBag.ClientInfo.VatNumber">
                                                        </div>
                                                    </div><!-- end row -->


                                                    <div class="row mb-3">
                                                        <label for="" class="col-md-2 col-form-label" style="width:fit-content">Address:</label>
                                                        <div class="col-md-9 input-group">
                                                            <span class="input-group-text material-symbols-outlined"> home_work</span>
                                                            <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default" value="@ViewBag.ClientInfo.OfficeAddress">
                                                        </div>
                                                    </div><!-- end row -->



                                                    <div class="row mb-3">
                                                        <label for="example-time-input" class="col-md-2 col-form-label" style="width:fit-content">Postal Code:</label>
                                                        <div class="col-md-9 input-group">
                                                            <span class="input-group-text material-symbols-outlined"> local_post_office</span>
                                                            <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default" value="@ViewBag.ClientInfo.PostalCode">
                                                        </div>
                                                    </div><!-- end row -->



                                                    <div class="row mb-3">
                                                        <div class="col-md-10">

                                                        </div>
                                                    </div><!-- end row -->
                                                    <div class="row mb-3">

                                                    </div><!-- end row -->
                                                    <div class="row mb-3">

                                                        <div class="col-md-10" style="padding-top:128px">

                                                        </div>

                                                    </div><!-- end row -->
                                                    <div class="row mb-3">
                                                        <label for="example-time-input" class="col-md-2 col-form-label" style="width:fit-content">Name:</label>
                                                        <div class="col-md-9 input-group">
                                                            <span class="input-group-text material-symbols-outlined"> person</span>
                                                            <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default" value="@ViewBag.ClientRep.FirstName @ViewBag.ClientRep.LastName" readonly>
                                                        </div>
                                                    </div><!-- end row -->



                                                    <div class="row mb-3">
                                                        <label for="example-time-input" class="col-md-2 col-form-label" style="width:fit-content">Email Address:</label>
                                                        <div class="col-md-9 input-group">
                                                            <span class="input-group-text material-symbols-outlined"> mail</span>
                                                            <input type="email" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default" value="@ViewBag.ClientRep.EmailAddress" readonly>
                                                        </div>
                                                    </div><!-- end row -->









                                                </div><!-- end col -->
                                            </div><!-- end row -->
                                        </div><!-- end card body -->
                                    </div><!-- end tab pane -->
                                    <div class="tab-pane" id="navtabs-profile" role="tabpanel">
                                        <div class="card-body">
                                            <div class="row">

                                                @using (Html.BeginForm("OnboardEntityUser", "Onboarding", FormMethod.Post))
                                                {
                                                    @Html.AntiForgeryToken()

                                                    <div class="text-center mb-4">
                                                        <h5>Onboard Entity User</h5>
                                                        <p class="card-title-desc">Enter all the below fiels to submit</p>
                                                    </div>
                                                    @Html.HiddenFor(model => model.Id, new { @Name = "entityId" })
                                                    <div>
                                                        <div class="row">
                                                            <div class="col-lg-6">
                                                               
                                                                    <div class="form-group mb-3">
                                                                        <div class="input-group">
                                                                            <span class="input-group-text material-symbols-outlined"> person </span>
                                                                            <input type="text" class="form-control" name="Firstname" id="basicpill-firstname-input" required value="First name">
                                                                        </div>
                                                                    </div>

                                                               

                                                            </div><!-- end col -->
                                                            <div class="col-lg-6">
                                                                <div class="mb-3">
                                                                    <div class="input-group">
                                                                        <span class="input-group-text material-symbols-outlined"> group </span>
                                                                        <input type="text" class="form-control" name="LastName" id="basicpill-lastname-input" required value="Last Name">
                                                                    </div>
                                                                </div>

                                                            </div><!-- end col -->
                                                        </div><!-- end row -->

                                                        <div class="row">
                                                            <div class="col-lg-6">
                                                                <div class="mb-3">
                                                                    <div class="input-group">
                                                                        <span class="input-group-text material-symbols-outlined"> call </span>
                                                                        <input type="text" class="form-control" name="Phone" id="basicpill-phoneno-input" required value="Phone">
                                                                    </div>
                                                                </div>

                                                            </div><!-- end col -->
                                                            <div class="col-lg-6">
                                                                <div class="mb-3">
                                                                    <div class="input-group">
                                                                        <span class="input-group-text material-symbols-outlined"> mail </span>
                                                                        <input type="email" class="form-control" name="Email" id="basicpill-email-input" required value="Email">
                                                                    </div>
                                                                </div>

                                                            </div><!-- end col -->
                                                        </div><!-- end row -->

                                                        <div class="row">
                                                            <div class="col-lg-6">
                                                                <div class="mb-3">
                                                                    <div class="input-group">
                                                                        <span class="input-group-text material-symbols-outlined"> settings_accessibility </span>
                                                                      
                                                                        <select class="form-select" name="Title" required>
                                                                            <option value="" selected>Select Title</option>
                                                                            @foreach (var title in ViewBag.PossibleTitles as List<string>)
                                                                            {
                                                                                <option value="@title">@title</option>
                                                                            }
                                                                        </select>
                                                                    </div>
                                                                </div>

                                                            </div><!-- end col -->
                                                            <div class="col-lg-6">
                                                                <div class="mb-3">
                                                                    <div class="input-group">
                                                                        <span class="input-group-text material-symbols-outlined"> badge </span>
                                                                        <input type="text" class="form-control" name="Designation" required placeholder="Designation">
                                                                    </div>
                                                                </div>


                                                            </div><!-- end col -->
                                                        </div><!-- end row -->

                                                        <div class="row">
                                                            <div class="col-lg-6">
                                                                <div>
                                                                    <button type="submit" class="btn btn-primary">Submit <span class="fa fa-pencil-alt"></span> </button>
                                                                    @*<input type="submit" class="btn btn-primary" id="submitFormBtn" />*@
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                }






                                            </div><!-- end row -->
                                        </div><!-- end card body -->
                                    </div><!-- end tab pane -->
                                    <div class="tab-pane" id="navtabs-messages" role="tabpanel">
                                        <div class="card-body">
                                            <div class="row">
                                                <div class="col-xl-8 mx-auto">
                                                    <h2 class="text-center mb-4">XET EMPLOYEES</h2>
                                                    <div class="table-responsive mb-4">
                                                        <table class="table table-hover table-bordered table-striped w-100 table-sm">
                                                            <thead class="table-dark">
                                                                <tr>
                                                                    <th>Title</th>
                                                                    <th>Name</th>
                                                                    <th>Email</th>
                                                                    <th>Contact Number</th>
                                                                    <th>Designation</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                @foreach (var user in ViewBag.ClientUserList)
                                                                {
                                                                    <tr>
                                                                        <td>@user.Title</td>
                                                                        <td>@user.FirstName @user.LastName</td>
                                                                        <td>@user.EmailAddress</td>
                                                                        <td>@user.MobileNumber</td>
                                                                        <td>@user.Designation</td>
                                                                    </tr>
                                                                }
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div><!-- end col -->
                                            </div><!-- end row -->
                                        </div><!-- end card body -->

                                    </div><!-- end tab pane -->
                                </div>
                            </div> <!-- end card body -->
                        </div> <!-- end card -->
                    </div> <!-- end col -->

                </div><!-- end row -->

            </div> <!-- container-fluid -->
        </div>
    </div>

    <div class="rightbar-overlay"></div>
    <script src="~/assets/js/pages/form-wizard.init.js"></script>
    <script src="~/assets/js/pages/form-advanced.init.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

</body>
</html>



<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<!-- Include the flatpickr library from CDN -->
<script src="https://cdn.jsdelivr.net/npm/flatpickr"></script>